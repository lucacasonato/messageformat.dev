{
  "imports": {
    "lume/": "https://cdn.jsdelivr.net/gh/lumeland/lume@326ff7e33a0a8056b95dabd2dbdf08e9ec7901d5/",
    "preact": "npm:preact"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
